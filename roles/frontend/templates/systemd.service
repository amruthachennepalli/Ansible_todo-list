[Unit]
Description = frontend Service
[Service]
User=root
Environment=AUTH_API_ADDRESS=http://login-dev.zsdevops.online:808
Environment=TODO_API_ADDRESS=http://todo-dev.zsdevops.online:808
ExecStart=/bin/node /var/www/html/todoapp/frontend/build/dev-server.js
Restart=on-failure
SyslogIdentifier=frontend

[Install]
WantedBy=multi-user.target